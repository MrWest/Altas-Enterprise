<UserControl x:Class="CompanyName.Atlas.Investments.Presentation.Views.ReportViewsUI.InvestmentFullReport"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CompanyName.Atlas.Investments.Presentation.Views.ReportViewsUI"
             xmlns:properties="clr-namespace:CompanyName.Atlas.Investments.Properties"
             xmlns:uiControls="clr-namespace:CompanyName.Atlas.UIControls;assembly=Atlas.UIControls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
              Background="{DynamicResource DataGrid.Static.Background}" 
             FontSize="{DynamicResource Default.FontSize}" FontFamily="{DynamicResource Default.FontFamily}" Foreground="{DynamicResource DataGrid.Static.Foreground}"
             Loaded="InvestmentFullReport_OnLoaded">
   
    <Grid x:Name="MyGrid">
        <FlowDocumentPageViewer x:Name="MyDocumentPageViewer"  IsVisibleChanged="UIElement_OnIsVisibleChanged" DataContextChanged="FrameworkElement_OnDataContextChanged">
            <FlowDocument x:Name="FlowDocument"   FontSize="{DynamicResource Default.FontSize}" FontFamily="{DynamicResource Default.FontFamily}" Foreground="{DynamicResource DataGrid.Static.Foreground}"
                          IsColumnWidthFlexible="True" IsOptimalParagraphEnabled="True" IsHyphenationEnabled="True"
                          ColumnWidth="{Binding ElementName=MyGrid,Path=ActualWidth}" >
               
            </FlowDocument>
        </FlowDocumentPageViewer>
